  font-weight: 400;
}

@font-face {
  font-family: 'Uniform-New';
  src: url("{{ 'Uniform-Condensed.ttf' | asset_url }}") format('truetype');
  font-weight: 500;
}

@font-face {
  font-family: 'Uniform-New';
  src: url("{{ 'Uniform-Condensed-Medium.ttf' | asset_url }}") format('truetype');
  font-weight: 600;
}

@font-face {
  font-family: 'Uniform-New';
  src: url("{{ 'Uniform-Condensed-Bold.ttf' | asset_url }}") format('truetype');
