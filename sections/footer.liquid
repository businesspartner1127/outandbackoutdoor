    {%- assign col_mob_class = "col-12" -%}
  {%- when 3 -%}
    {%- assign col_mob_class = "col-4" -%}
{%- endcase -%}

{%- assign col_tablet_class = "col-md-4" -%}
{%- case section.settings.col_count_tablet -%}
