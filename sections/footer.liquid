    {%- assign col_tablet_class = "col-md-6" -%}
  {%- when 4 -%}
    {%- assign col_tablet_class = "col-md-3" -%}
{%- endcase -%}

{%- assign col_desktop_class = "col-xl-4" -%}
{%- case section.settings.col_count_desktop -%}
  {%- when 2 -%}
    {%- assign col_desktop_class = "col-xl-6" -%}
  {%- when 4 -%}
    {%- assign col_desktop_class = "col-xl-3" -%}
{%- endcase -%}

<style>
  
  .new_footer {
    background: {{ section.settings.back_color }};
  }

  .footer_header_col h1, .footer-ul-header, .footer_col a, .footer_col li, .social_icons .fa, .copyright_col p, .footer_col a:hover, .footer_col a:focus {
    color: {{ section.settings.text_color }}
  }
  
</style>


