{% comment %}
  DO NOT MODIFY THIS FILE DIRECTLY. THIS FILE WILL BE OVERWRITTEN.
  YOU CAN MODIFY THE LIQUID CODE FOR THIS CUSTOM FIELD HERE:
  https://customfields.bonify.io/dashboard/out-and-back-outdoor.myshopify.com/products/configure/249804/edit
{% endcomment %}
{% if product.metafields.custom_fields["found_details_colors"] != blank %}
  <div class="custom-field custom-field__found-details-colors custom-field__type--text-list">
    <h3 class="custom-field--title">Found Details Colors</h3>
    <div class="custom-field--value">  
      {% assign values = product.metafields.custom_fields["found_details_colors"] | split: "|" %}
      <ul>
      {% for value in values %}
        <li>{{ value }}</li>
      {% endfor %}
      </ul>
    </div>
  </div>
{% endif %}